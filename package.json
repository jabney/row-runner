{
  "name": "row-runner",
  "version": "0.1.4",
  "description": "Process csv files as streams of rows",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "cover": "npm run clean && tap --ts --no-check-coverage --coverage-report=html",
    "clean": "rm -rf dist",
    "prepare": "npm run test-ts && npm run build",
    "test": "npm run build && tap --ts --no-coverage",
    "test-ts": "npm run clean && tap --ts --no-check-coverage"
  },
  "author": "James Abney",
  "license": "ISC",
  "keywords": [
    "csv",
    "stream",
    "etl",
    "row",
    "json"
  ],
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@types/tap": "^15.0.7",
    "tap": "^16.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "seedable": "^1.0.5"
  }
}
